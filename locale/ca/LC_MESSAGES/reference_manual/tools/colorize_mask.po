#: ../../reference_manual/tools/colorize_mask.rst:16
msgid "Colorize Mask"
msgstr "Màscara d'acoloriment"

#: ../../reference_manual/tools/colorize_mask.rst:11
msgid "Tools"
msgstr "Eines"

#: ../../reference_manual/tools/colorize_mask.rst:11
msgid "Lazybrush"
msgstr "Pinzell menys estricte"

