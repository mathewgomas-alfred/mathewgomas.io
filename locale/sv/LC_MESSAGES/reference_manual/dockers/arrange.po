msgstr "Höj"

#: ../../reference_manual/dockers/arrange.rst:49
msgid "Lower"
msgstr "Sänk"

#: ../../reference_manual/dockers/arrange.rst:50
msgid "Order"
msgstr "Ordning"

#: ../../reference_manual/dockers/arrange.rst:50
msgid "Bring to back"
msgstr "Flytta underst"

