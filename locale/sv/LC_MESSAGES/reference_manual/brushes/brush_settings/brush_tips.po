#: ../../reference_manual/brushes/brush_settings/brush_tips.rst:12
msgid "Brush tip"
msgstr "Penselspets"

#: ../../reference_manual/brushes/brush_settings/brush_tips.rst:12
msgid "Brush Mask"
msgstr "Penselmask"

#: ../../reference_manual/brushes/brush_settings/brush_tips.rst:18
msgid "Brush Tips"
msgstr "Penselspetsar"

#: ../../reference_manual/brushes/brush_settings/brush_tips.rst:22
