#: ../../reference_manual/tools/move.rst:11
msgid "Tools"
msgstr "Ferramentas"

#: ../../reference_manual/tools/move.rst:11
msgid "Move"
msgstr "Mover"

#: ../../reference_manual/tools/move.rst:11
